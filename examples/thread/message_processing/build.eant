<?xml version="1.0"?>

<project name="message_processing" default="help">

	<description>
		description: "Stand alone servlet Example"
		project: "Project Goanna [http://sourceforge.net/projects/goanna]"
		date: "$Date$"
		revision: "$Revision$"
		author: "Colin Adams [colin-adams@users.sourceforge.net]"
		copyright: "Copyright (c) 2005 Colin Adams and others"
		license: "Eiffel Forum License v2 (see forum.txt)."
	</description>

	<inherit>
		<parent location="${GOBO}/misc/eiffel.eant">
			<redefine target="init_system"/>
			<redefine target="help"/>
		</parent>
	</inherit>

	<target name="help">
		<echo message="usage:"/>
		<echo message="   geant install"/>
		<echo message="   geant compile"/>
		<echo message="   geant compile_ise"/>
		<echo message="   geant compile_se"/>
		<echo message="   geant compile_ve"/>
		<echo message="   geant compile_debug"/>
		<echo message="   geant compile_debug_ise"/>
		<echo message="   geant compile_debug_se"/>
		<echo message="   geant compile_debug_ve"/>
		<echo message="   geant clean"/>
		<echo message="   geant clobber"/>
	</target>

<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="message_processing"/>
		<set name="library" value="thread"/>
		<set name="system_dir" value="${GOANNA}/examples/${library}/${system}"/>
	</target>

</project>
