system
	goanna

root
	GOANNA: make

default
	assertion(check)
	assertion(require)
	assertion(ensure)
	assertion(loop)
	assertion(invariant)
	arguments(" ")
	disabled_debug(yes)
	disabled_debug("base64_encode")
	disabled_debug("dom_assertions")
	debug("log4e_config")
	disabled_debug("namespace")
	disabled_debug("normalize")
	disabled_debug("parser_events")
	disabled_debug("unhandled_node_types")
	line_generation(no)
	profile(no)
	trace(no)
	il_verifiable(yes)
	msil_generation_type("exe")
	check_vape(yes)
	console_application(yes)
	address_expression(no)
	array_optimization(no)
	dead_code_removal(yes)
	dynamic_runtime(no)
	exception_trace(no)
	inlining(no)
	multithreaded(no)


-- Gobo Eiffel Kernel Library

cluster
	goanna_utility:		"$GOANNA\library\utility"


	goanna_utility_ise (goanna_utility):		"$\spec\ise"

	goanna_log4e:		"$GOANNA\library\log4e"

	log4e_appenders (goanna_log4e):		"$\appenders"

	log4e_nt_appenders (log4e_appenders):		"$\nt"

	log4e_net_appenders (log4e_appenders):		"$\net"

	log4e_filters (goanna_log4e):		"$\filters"

	log4e_helpers (goanna_log4e):		"$\helpers"

	log4e_layouts (goanna_log4e):		"$\layouts"

	log4e_pattern (log4e_layouts):		"$\pattern"

	log4e_config (goanna_log4e):		"$\config"

	all base:		"$ISE_EIFFEL/library/base"
		exclude
			"desc"; "table_eiffel3"; "CVS"; "EIFGEN"; 
		end

	utility_tests:		"$GOANNA\test\utility"

	log4e_tests:		"$GOANNA\test\log4e"

	testgen:		"$GOANNA\test\TESTGEN"

	yaesockets:		"$YAESOCKETS"

	yaesockets_ise (yaesockets):		"$\spec\ise"

	all gobo:		"$GOBO/library"
		exclude
			"other"; "hact"; "se"; "ve"; 
		end

	dom:		"$GOANNA\library\dom"

	dom_core (dom):		"$\core"

	dom_traversal_range (dom):		"$\traversal-range"

	dom_impl (dom):		"$\implementation"

	dom_impl_core (dom_impl):		"$\core"

	dom_impl_traversal_range (dom_impl):		"$\traversal-range"

	dom_writer (dom):		"$\writer"

	xml_parser:		"$GOANNA\library\xml_parser"

external

	include_path:
		"$EXPAT\lib",
		"$GOBO\library\kernel\include",
		"$GOBO\library\time\include",
		"$GOANNA\library\log4e\appenders\nt\lib"

	object:
		"$YAESOCKETS\C\Release\cl.lib",
		"$EXPAT\lib\Debug\expat.lib",
		"$EXML/library/impl/expat/spec/c/libexml.lib"

end
