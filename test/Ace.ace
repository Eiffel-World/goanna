system
	goanna

root
	GOANNA: make

default
	assertion(check)
	assertion(require)
	assertion(ensure)
	assertion(loop)
	assertion(invariant)
	arguments(" ")
	disabled_debug(yes)
	disabled_debug("base64_encode")
	debug(no)
	line_generation(no)
	profile(no)
	trace(no)
	il_verifiable(yes)
	msil_generation_type("exe")
	check_vape(yes)
	console_application(yes)
	address_expression(no)
	array_optimization(no)
	dead_code_removal(yes)
	dynamic_runtime(no)
	exception_trace(no)
	inlining(no)
	multithreaded(no)


cluster
	goanna_utility:		"$GOANNA\library\utility"


	goanna_utility_ise (goanna_utility):		"$\spec\ise"

	goanna_log4e:		"$GOANNA\library\log4e"

	log4e_appenders (goanna_log4e):		"$\appenders"

	log4e_nt_appenders (log4e_appenders):		"$\nt"

	log4e_net_appenders (log4e_appenders):		"$\net"

	log4e_filters (goanna_log4e):		"$\filters"

	log4e_helpers (goanna_log4e):		"$\helpers"

	log4e_layouts (goanna_log4e):		"$\layouts"

	log4e_pattern (log4e_layouts):		"$\pattern"

	all base:		"$ISE_EIFFEL/library/base"
		exclude
			"desc"; "table_eiffel3"; "CVS"; "EIFGEN"; 
		end

	utility_tests:		"$GOANNA\test\utility"

	log4e_tests:		"$GOANNA\test\log4e"

	testgen:		"$GOANNA\test\TESTGEN"

	yaesockets:		"$YAESOCKETS"

	yaesockets_ise (yaesockets):		"$\spec\ise"

		-- Gobo Eiffel Kernel Library
	gobo:				"$GOBO/library";
	kl_kernel (gobo):			"$/kernel";
	kl_io (gobo):			"$/kernel/io";
	kl_misc (gobo):			"$/kernel/misc";
	kl_unicode (gobo):		"$/kernel/unicode";
	kl_kernel_spec (gobo):		"$/kernel/spec/ise";
--	kl_kernel_os (gobo):		"$/kernel/spec/$GOBO_OS";
		-- Gobo Eiffel Structure Library
	ds_container (gobo):		"$/structure/container";
	ds_dispenser (gobo):		"$/structure/dispenser";
	ds_list (gobo):			"$/structure/list";
	ds_set (gobo):			"$/structure/set";
	ds_sort (gobo):			"$/structure/sort";
	ds_support (gobo):		"$/structure/support";
	ds_table (gobo):			"$/structure/table";
		-- Gobo Eiffel Test Library
	ts_generation (gobo):		"$/test/generation";
	ts_harness (gobo):		"$/test/harness";
		-- Gobo Eiffel Time Library
	dt_clock (gobo):			"$/time/clock";
	dt_date (gobo):			"$/time/date";
	dt_spec (gobo):			"$/time/spec/ise";
	dt_time_zone (gobo):		"$/time/time_zone";
			-- Gobo Eiffel Utility Library
	ut_command (gobo):		"$/utility/command";
	ut_error (gobo):			"$/utility/error";
	ut_formatter (gobo):		"$/utility/formatter";
	ut_support (gobo):		"$/utility/support";
	ut_spec (gobo):			"$/utility/spec/ise";

external

	include_path:
		"$GOBO\library\kernel\include",
		"$GOBO\library\time\include",
		"$GOANNA\library\log4e\appenders\nt\lib"

	object:
		"$YAESOCKETS\C\Release\cl.lib"

end
