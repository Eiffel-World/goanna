<?xml version="1.0"?>

<project name="utility" default="help">

	<description>
		system:      "Goanna Utility Library Tester"
		project: "Project Goanna [http://sourceforge.net/projects/goanna]"
		date: "$Date$"
		revision: "$Revision$"
		author: "Glenn Maughan [glennmaughan@optushome.com.au]"
		copyright: "Copyright (c) 2001 Glenn Maughan and others"
		license: "Eiffel Forum Freeware License v1 (see forum.txt)."
	</description>

	<inherit>
		<parent location="${GOBO}/misc/test.eant">
		  <redefine target="init_system"/>
		</parent>
	</inherit>

<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="xutility"/>
		<set name="library" value="utility"/>
		<set name="system_dir" value="${GOANNA}/test/${library}"/>
	</target>

</project>
